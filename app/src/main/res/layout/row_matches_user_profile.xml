<?xml version="1.0" encoding="utf-8"?>
<com.weball.benoit.weball.custom.CustomLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/five_row"
    android:layout_height="80dp"
    android:focusable="false"
    android:layout_width="100dp"
    android:padding="2dp"
    >

    <ImageView
        android:id="@+id/five_picture"
        style="@style/BackGroundImageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="false"
        android:descendantFocusability="blocksDescendants"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/search_image"
        android:scaleType="fitXY"
        android:background="@drawable/img"/>

    <TextView
        android:id="@+id/state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:textIsSelectable="false"
        android:layout_centerInParent="true"
        android:textSize="16sp"
        android:textColor="@android:color/white"

        />


</com.weball.benoit.weball.custom.CustomLayout>